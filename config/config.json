{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "carosuel-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/carosuel/CarosuelWebPart.js",
          "manifest": "./src/webparts/carosuel/CarosuelWebPart.manifest.json"
        }
      ]
    },
    "quick-links-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/quickLinks/QuickLinksWebPart.js",
          "manifest": "./src/webparts/quickLinks/QuickLinksWebPart.manifest.json"
        }
      ]
    },
    "footer-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/footer/FooterWebPart.js",
          "manifest": "./src/webparts/footer/FooterWebPart.manifest.json"
        }
      ]
    },
    "news-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/news/NewsWebPart.js",
          "manifest": "./src/webparts/news/NewsWebPart.manifest.json"
        }
      ]
    },
    "announce-ments-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/announceMents/AnnounceMentsWebPart.js",
          "manifest": "./src/webparts/announceMents/AnnounceMentsWebPart.manifest.json"
        }
      ]
