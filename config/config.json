      ]
    },
    "map-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/map/MapWebPart.js",
          "manifest": "./src/webparts/map/MapWebPart.manifest.json"
        }
      ]
    },
    "calendar-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/calendar/CalendarWebPart.js",
          "manifest": "./src/webparts/calendar/CalendarWebPart.manifest.json"
        }
      ]
    },
    "full-calendarview-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/fullCalendarview/FullCalendarviewWebPart.js",
          "manifest": "./src/webparts/fullCalendarview/FullCalendarviewWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CarosuelWebPartStrings": "lib/webparts/carosuel/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "QuickLinksWebPartStrings": "lib/webparts/quickLinks/loc/{locale}.js",
    "FooterWebPartStrings": "lib/webparts/footer/loc/{locale}.js",
    "NewsWebPartStrings": "lib/webparts/news/loc/{locale}.js",
    "AnnounceMentsWebPartStrings": "lib/webparts/announceMents/loc/{locale}.js",
    "ImageCarosuelWebPartStrings": "lib/webparts/imageCarosuel/loc/{locale}.js",
    "HeaderWebPartStrings": "lib/webparts/header/loc/{locale}.js",
    "AnnouncementviewWebPartStrings": "lib/webparts/announcementview/loc/{locale}.js",
    "NewsviewWebPartStrings": "lib/webparts/newsview/loc/{locale}.js",
    "MapWebPartStrings": "lib/webparts/map/loc/{locale}.js",
    "CalendarWebPartStrings": "lib/webparts/calendar/loc/{locale}.js",
    "FullCalendarviewWebPartStrings": "lib/webparts/fullCalendarview/loc/{locale}.js"
