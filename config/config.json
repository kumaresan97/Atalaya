    "image-carosuel-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/imageCarosuel/ImageCarosuelWebPart.js",
          "manifest": "./src/webparts/imageCarosuel/ImageCarosuelWebPart.manifest.json"
        }
      ]
    },
    "header-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/header/HeaderWebPart.js",
          "manifest": "./src/webparts/header/HeaderWebPart.manifest.json"
        }
      ]
    },
    "announcementview-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/announcementview/AnnouncementviewWebPart.js",
          "manifest": "./src/webparts/announcementview/AnnouncementviewWebPart.manifest.json"
        }
      ]
    },
    "newsview-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/newsview/NewsviewWebPart.js",
          "manifest": "./src/webparts/newsview/NewsviewWebPart.manifest.json"
